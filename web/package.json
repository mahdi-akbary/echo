{
  "name": "shopify-app-template-node",
  "version": "1.0.0",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "debug": "node --inspect-brk index.js",
    "dev": "cross-env NODE_ENV=development nodemon index.js --ignore ./frontend",
    "serve": "cross-env NODE_ENV=production node index.js",
    "supabase": "supabase",
    "supabase:link": "supabase link --project-ref $(grep SUPAbASE_PROJECT_REF .env.local | cut -d '=' -f2) -p $(grep SUPAbASE_PROJECT_DB_PASSWORD .env.local | cut -d '=' -f2)",
    "make:migration": "supabase migration new",
    "migrate": "supabase db push",
    "db:reset": "supabase db reset --linked"
  },
  "type": "module",
  "engines": {
    "node": ">=14.13.1"
  },
  "dependencies": {
    "@shopify/shopify-app-express": "^2.1.1",
    "@shopify/shopify-app-session-storage-sqlite": "^1.2.2",
    "@supabase/supabase-js": "^2.25.0",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "serve-static": "^1.14.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.15",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.3"
  }
}
